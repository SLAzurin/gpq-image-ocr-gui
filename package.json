{
  "name": "gpq-image-ocr-gui",
  "version": "1.1.0",
  "description": "Maplestory GPQ score screenshots to JSON",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "dev:css": "tailwindcss -i ./tailwind.css -o ./src/output.css --watch",
    "build": "tailwindcss -i ./tailwind.css -o ./src/output.css && vue-tsc --noEmit && vite build && electron-builder --config ./electron-builder-config.yml -p never",
    "preview": "vite preview"
  },
  "author": "SLAzurin",
  "license": "MIT",
  "dependencies": {
    "@highlightjs/vue-plugin": "^2.1.2",
    "axios": "^1.6.8",
    "highlight.js": "^11.9.0"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "engine": {
    "node": ">=20",
    "pnpm": "^8.15.1"
  },
  "type": "module",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "daisyui": "^4.10.2",
    "electron": "^28.3.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.25.0",
    "tailwindcss": "^3.4.3",
    "tree-kill": "^1.2.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.24",
    "vue-tsc": "^2.0.14"
  },
  "packageManager": "pnpm@9.0.5+sha256.61bd66913b52012107ec25a6ee4d6a161021ab99e04f6acee3aa50d0e34b4af9"
}