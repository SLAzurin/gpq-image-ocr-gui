{
  "name": "gpq-image-ocr-gui",
  "version": "1.4.1",
  "description": "Maplestory GPQ score screenshots to JSON",
  "main": "dist-electron/main/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SLAzurin/gpq-image-ocr-gui.git"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "dev:css": "tailwindcss -i ./tailwind.css -o ./src/output.css --watch",
    "build": "tailwindcss -i ./tailwind.css -o ./src/output.css && vue-tsc --noEmit && vite build && electron-builder --config ./electron-builder-config.yml -p never",
    "preview": "vite preview"
  },
  "author": "SLAzurin",
  "license": "MIT",
  "dependencies": {
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/theme-one-dark": "^6.1.2",
    "axios": "^1.6.8",
    "codemirror": "^6.0.1",
    "electron-updater": "^6.3.9",
    "vue-codemirror": "^6.1.1"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "engine": {
    "node": ">=20",
    "pnpm": "^8.15.1"
  },
  "type": "module",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "daisyui": "^4.11.1",
    "electron": "^30.0.3",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-config-love": "^47.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.26.0",
    "tailwindcss": "^3.4.3",
    "tree-kill": "^1.2.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.27",
    "vue-tsc": "^2.0.17"
  },
  "packageManager": "pnpm@10.6.5+sha512.cdf928fca20832cd59ec53826492b7dc25dc524d4370b6b4adbf65803d32efaa6c1c88147c0ae4e8d579a6c9eec715757b50d4fa35eea179d868eada4ed043af",
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "electron-winstaller",
      "esbuild"
    ]
  }
}