{
  "name": "gpq-image-ocr-gui",
  "version": "1.0.0",
  "description": "Maplestory GPQ score screenshots to JSON",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "dev:css": "tailwindcss -i ./tailwind.css -o ./src/output.css --watch",
    "build": "tailwindcss -i ./tailwind.css -o ./src/output.css && vue-tsc --noEmit && vite build && electron-builder --config ./electron-builder-config.yml",
    "preview": "vite preview"
  },
  "author": "SLAzurin",
  "license": "MIT",
  "dependencies": {
    "@highlightjs/vue-plugin": "^2.1.0",
    "axios": "^1.6.7",
    "highlight.js": "^11.9.0"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "engine": {
    "npm": "<=1"
  },
  "type": "module",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "daisyui": "^4.6.1",
    "electron": "^28.2.1",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.21.0",
    "tailwindcss": "^3.4.1",
    "tree-kill": "^1.2.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.15",
    "vue-tsc": "^1.8.27"
  }
}